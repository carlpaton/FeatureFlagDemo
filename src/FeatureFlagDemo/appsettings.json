{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "FeatureManagement": {
    "NewFeatureFlag": true,
    "HappyHolidaysFlag": {
      "EnabledFor": [
        {
          "Name": "TimeWindow",
          "Parameters": {
            "Start": "9 Aug 2021 09:10:00 +12:00",
            "End": "9 Aug 2021 09:11:00 +12:00"
          }
        }
      ]
    },
    "BrowserFlag": {
      "EnabledFor": [
        {
          "Name": "BrowserFeatureFilter",
          "Parameters": {
            "AllowedBrowsers": [
              "Firefox"
            ]
          }
        }
      ]
    },
    "RandomFlag": {
      "EnabledFor": [
        {
          "Name": "Percentage",
          "Parameters": {
            "Value": 50
          }
        }
      ]
    }
  }
}
